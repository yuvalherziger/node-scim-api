{
  "name": "scim-api",
  "version": "0.1.0",
  "description": "A SCIM API implementation in Node.js with a MongoDB backend",
  "keywords": [
    "scim"
  ],
  "scripts": {
    "test": "jest",
    "test-cov": "jest --coverage",
    "build": "tsc",
    "start": "node dist/index.js",
    "start:listener": "node dist/hooks/listener.js",
    "dev": "tsx watch src/index.ts",
    "test-scim-compliance": "node compliance-test/index.js",
    "migrate": "node dist/migrations/scripts/migrate.js",
    "generate-migration": "node dist/migrations/scripts/generate-migration.js --name ${npm_config_name:=default}",
    "build-and-migrate": "npm run build && npm run migrate",
    "build-and-generate-migration": "npm run build && npm run generate-migration"
  },
  "homepage": "https://github.com/yuvalherziger/node-scim-api#readme",
  "bugs": {
    "url": "https://github.com/yuvalherziger/node-scim-api/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yuvalherziger/node-scim-api.git"
  },
  "license": "MIT",
  "author": "Yuval Herziger <yuvalhrz@gmail.com>",
  "type": "module",
  "main": "src/index.ts",
  "dependencies": {
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "express-winston": "^4.2.0",
    "mongodb": "^6.20.0",
    "winston": "^3.18.3"
  },
  "devDependencies": {
    "@types/ejs": "^3.1.5",
    "@types/express": "^5.0.4",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.9.1",
    "@types/supertest": "^2.0.15",
    "axios": "^1.12.2",
    "ejs": "^3.1.10",
    "jest": "^29.7.0",
    "supertest": "^6.3.4",
    "ts-jest": "^29.4.5",
    "ts-node-dev": "^2.0.0",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3"
  }
}
